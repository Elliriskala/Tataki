{
    "tags": {
      "allowUnknownTags": true
    },
    "source": {
      "include": ["js-src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(^|\\/|\\\\)_"
    },
    "opts": {
      "destination": "./jsdocs",
      "recurse": true
    },
    "templates": {
      "cleverLinks": true,
      "monospaceLinks": true
    }
  }