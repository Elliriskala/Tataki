{
    "compilerOptions": {
      "moduleResolution": "node",
      "outDir": "./build",
      "target": "ESNext",
      "module": "ESNext",
      "noImplicitAny": true,
      "strict": true,
      "esModuleInterop": true,
      "typeRoots": ["./node_modules/@types", "./src/types"],
    },
    "include": ["./src/**/*"]
}